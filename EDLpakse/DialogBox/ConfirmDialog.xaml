<Window x:Class="EDLpakse.DialogBox.ConfirmDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EDLpakse.DialogBox"
        mc:Ignorable="d"
        Height="149.766" Width="300" WindowStartupLocation="CenterScreen"  Background="{StaticResource MenuItemSelectionFill}" Icon="/EDLpakse;component/ICON.ico" WindowStyle="ToolWindow" ResizeMode="NoResize">
    <Grid>
        <TextBlock x:Name="label1" Margin="10,21,10,0" FontSize="14" Foreground="#FFDA7D0E" TextAlignment="Center" FontFamily="Phetsarath OT" Height="40" VerticalAlignment="Top" FontWeight="Bold"/>
        <Button x:Name="OK" Content="OK" Margin="10,0,0,10"  Click="OK_Click" HorizontalAlignment="Left" Width="122" Height="30" VerticalAlignment="Bottom"/>
        <Button x:Name="Cancel" Content="Cancel" Margin="0,0,10,10" Height="30" VerticalAlignment="Bottom" Click="Cancel_Click" HorizontalAlignment="Right" Width="122"/>
    </Grid>
</Window>
